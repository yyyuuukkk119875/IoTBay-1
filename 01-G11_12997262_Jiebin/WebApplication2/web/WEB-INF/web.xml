<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ConnServlet</servlet-name>
        <servlet-class>ISD.Assignment.Controller.ConnServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>shipmentSearchServlet</servlet-name>
        <servlet-class>ISD.Assignment.Controller.shipmentSearchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShipmentEditServlet</servlet-name>
        <servlet-class>ISD.Assignment.Controller.ShipmentEditServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>shipmentUpdateServlet</servlet-name>
        <servlet-class>ISD.Assignment.Controller.shipmentUpdateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShipmentDeleteServlet</servlet-name>
        <servlet-class>ISD.Assignment.Controller.ShipmentDeleteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AllShipmentsServlet</servlet-name>
        <servlet-class>ISD.Assignment.Controller.AllShipmentsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewShipmentServlet</servlet-name>
        <servlet-class>ISD.Assignment.Controller.NewShipmentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ConnServlet</servlet-name>
        <url-pattern>/ConnServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>shipmentSearchServlet</servlet-name>
        <url-pattern>/shipmentSearchServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShipmentEditServlet</servlet-name>
        <url-pattern>/ShipmentEditServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>shipmentUpdateServlet</servlet-name>
        <url-pattern>/shipmentUpdateServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShipmentDeleteServlet</servlet-name>
        <url-pattern>/ShipmentDeleteServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AllShipmentsServlet</servlet-name>
        <url-pattern>/AllShipmentsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewShipmentServlet</servlet-name>
        <url-pattern>/NewShipmentServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
